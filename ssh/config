Host devbox.pagarme.net
	ControlMaster auto
	ControlPath /tmp/ssh_mux_%h_%p_%r
	ControlPersist 900s
	ForwardAgent yes
	GSSAPIAuthentication yes
	GSSAPIDelegateCredentials yes
	GSSAPITrustDNS no

Host gwx
	HostName gwx.gboxnet.ga
	ControlMaster auto
	ControlPath /tmp/ssh_mux_%h_%p_%r
	ControlPersist 900s
	ForwardAgent yes
	DynamicForward 2001
	User greenboxal

Host jumpbox
	HostName jumpbox.internal.pagar.me
	ControlMaster auto
	ControlPath /tmp/ssh_mux_%h_%p_%r
	ControlPersist 900s
	ForwardAgent yes

Host bunker
	HostName bunker.internal.pagar.me
	IdentityFile ~/.ssh/bunker
	ForwardAgent yes

Host *.aws.pagarme.net
	ProxyCommand ssh -W %h:%p jumpbox
	ForwardAgent yes
	GSSAPIAuthentication yes
	GSSAPIDelegateCredentials yes
	GSSAPITrustDNS no
	ForwardX11 yes
	ForwardX11Trusted yes

Host pg.*
	ProxyCommand ssh -W %h:%p jumpbox

